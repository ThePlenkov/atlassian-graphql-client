{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsdown",
        "cwd": "{projectRoot}"
      },
      "inputs": [
        "default",
        "^default",
        "{projectRoot}/tsdown.config.ts",
        "{workspaceRoot}/tsdown.config.mjs"
      ],
      "outputs": ["{projectRoot}/dist"],
      "cache": true,
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["{projectRoot}/coverage"],
      "cache": true
    },
    "lint": {
      "cache": true
    }
  },
  "nxCloudId": "68fe37fdf3953438877ebdcb",
  "plugins": [
    {
      "plugin": "@nx/js/typescript",
      "options": {
        "typecheck": {
          "targetName": "typecheck"
        }
      }
    }
  ]
}
