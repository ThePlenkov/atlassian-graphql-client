{
  "name": "gqlb",
  "version": "0.1.4",
  "description": "Runtime proxy-based GraphQL query builder with type safety",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./typed": {
      "import": "./dist/typed-builder.mjs",
      "types": "./dist/typed-builder.d.mts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "graphql",
    "query-builder",
    "typescript",
    "proxy",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gqlb/gqlb",
    "directory": "packages/gqlb"
  },
  "homepage": "https://github.com/gqlb/gqlb#readme",
  "bugs": {
    "url": "https://github.com/gqlb/gqlb/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.1.2"
  }
}
