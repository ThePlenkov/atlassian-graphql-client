{
  "name": "gqlb",
  "version": "0.1.0",
  "description": "Runtime proxy-based GraphQL query builder with type safety",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./typed": {
      "import": "./dist/typed-builder.mjs",
      "types": "./dist/typed-builder.d.mts"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "graphql",
    "query-builder",
    "typescript",
    "proxy",
    "type-safe"
  ],
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "graphql": "^16.11.0"
  }
}
