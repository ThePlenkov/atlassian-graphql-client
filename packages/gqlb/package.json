{
  "name": "gqlb",
  "version": "0.1.0",
  "description": "Runtime proxy-based GraphQL query builder with type safety",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./typed": {
      "import": "./dist/src/typed-builder.js",
      "require": "./dist/src/typed-builder.js",
      "types": "./dist/src/typed-builder.d.ts"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "graphql",
    "query-builder",
    "typescript",
    "proxy",
    "type-safe"
  ],
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "graphql": "^16.11.0"
  }
}
