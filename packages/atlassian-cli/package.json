{
  "name": "@atlassian-tools/cli",
  "version": "0.1.0",
  "description": "CLI for Atlassian APIs using GraphQL",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils/logger": {
      "import": "./dist/utils/logger.js",
      "types": "./dist/utils/logger.d.ts"
    },
    "./auth/config": {
      "import": "./dist/auth/config.js",
      "types": "./dist/auth/config.d.ts"
    }
  },
  "bin": {
    "atlassian": "./dist/cli.js",
    "jira": "./dist/jira-cli.js"
  },
  "keywords": [
    "atlassian",
    "jira",
    "confluence",
    "cli",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gqlb/gqlb",
    "directory": "packages/atlassian-cli"
  },
  "homepage": "https://github.com/gqlb/gqlb#readme",
  "bugs": {
    "url": "https://github.com/gqlb/gqlb/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@atlassian-tools/gql": "*",
    "cli-oauth": "*"
  }
}

